---
title: "{{ page.title }}"
{% if page.icon %}icon: "{{ page.icon }}"{% endif %}
description: "{{ page.description }}"
---

{% block breadcrumb %}
{% if page.breadcrumb and page.breadcrumb.size > 0 %}
{% for item in page.breadcrumb %}
{% unless forloop.first %}
  <span className="breadcrumb-separator">{' > '}</span>
{% endunless %}
{% if item.path %}
  [{{ item.name }}]({{ item.path }})
{% else %}
  <span className="breadcrumb-current">{{ item.name }}</span>
{% endif %}
{% endfor %}
{% endif %}
{% endblock %}

# {{ apiItem.displayName }}

{% block warnings %}
{% if apiItem.isDeprecated %}
<Warning>
  **Deprecated**: This {{ apiItem.kind | downcase }} is deprecated and may be removed in a future version.
</Warning>
{% endif %}

{% if apiItem.isAlpha %}
<Warning>
  **Alpha**: This {{ apiItem.kind | downcase }} is in alpha and may change without notice.
</Warning>
{% endif %}

{% if apiItem.isBeta %}
<Warning>
  **Beta**: This {{ apiItem.kind | downcase }} is in beta and may change without notice.
</Warning>
{% endif %}
{% endblock %}

{% block summary %}
{% if apiItem.summary %}
## Summary

{{ apiItem.summary }}
{% endif %}
{% endblock %}

{% block signature %}
{% if apiItem.signature %}
## Signature

```typescript
{{ apiItem.signature }}
```
{% endif %}
{% endblock %}

{% block heritage %}
{% if heritageTypes and heritageTypes.size > 0 %}
## Heritage

{% for type in heritageTypes %}
{% if type.path %}
* [{{ type.name }}]({{ type.path }})
{% else %}
* {{ type.name }}
{% endif %}
{% endfor %}
{% endif %}
{% endblock %}

{% block content %}{% endblock %}

{% block remarks %}
{% if apiItem.remarks %}
## Remarks

{{ apiItem.remarks }}
{% endif %}
{% endblock %}

{% block examples %}
{% if examples and examples.size > 0 %}
## Examples

{% for example in examples %}
### Example {{ forloop.index }}

```typescript
{{ example }}
```
{% endfor %}
{% endif %}
{% endblock %}